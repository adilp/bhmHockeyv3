{
  "expo": {
    "name": "BHM Hockey",
    "slug": "bhm-hockey",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0D1117"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.bhmhockey.app",
      "buildNumber": "7",
      "infoPlist": {
        "LSApplicationQueriesSchemes": [
          "venmo"
        ],
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "NSUserNotificationsUsageDescription": "BHM Hockey sends notifications about new events, registration updates, and payment reminders."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0D1117"
      },
      "package": "com.bhmhockey.app",
      "googleServicesFile": "./google-services.json",
      "versionCode": 2
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "bhmhockey",
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#00D9C0"
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f6225f7a-0181-4b8c-b44a-cea3e0017cfa"
      }
    },
    "owner": "adilp",
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/f6225f7a-0181-4b8c-b44a-cea3e0017cfa",
      "enabled": true,
      "checkOnLaunch": "ALWAYS",
      "fallbackToCacheTimeout": 5000
    }
  }
}
